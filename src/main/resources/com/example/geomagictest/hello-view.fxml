<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="1500.0" prefWidth="1500.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.geomagictest.GeoMagicTestController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <VBox fx:id="sidbarVBox" alignment="TOP_CENTER" maxWidth="500.0" minWidth="300.0" spacing="10.0" HBox.hgrow="ALWAYS">
      <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#loadDataButtonClick" text="Lade Daten" />
    </VBox>
    <VBox alignment="CENTER" maxHeight="1024.0" maxWidth="1024.0" style="-fx-background-color: #FFFFFF;" HBox.hgrow="ALWAYS">
      <children>
         <Canvas fx:id="polyLineCanvas" height="1024.0" width="1024.0" />
      </children>
    </VBox>
</HBox>
